{
  "name": "todo-mvc-react",
  "version": "0.1.0",
  "description": "An implementation of Todo MVC using React.",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bgoldman/todo-mvc-react"
  },
  "author": "Brandon Goldman",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "config": "^1.19.0",
    "express": "^4.13.4"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "markdown-live": "^1.0.8",
    "nodemon": "^1.8.1"
  },
  "engines": {
    "node": "^5.6.0"
  },
  "scripts": {
    "start": "nodemon server.js --ignore _build --exec babel-node",
    "start:build": "nodemon server.js --ignore _build --exec 'npm run build && cd _build && node'",
    "build": "npm run build:clean && npm run build:js",
    "build:clean": "rm -rf _build && mkdir _build",
    "build:js": "babel . --ignore=node_modules --out-dir=_build --source-maps=inline --copy-files",
    "lint": "npm run lint:js",
    "lint:js": "eslint src --fix",
    "preview:readme": "mdlive *.md",
    "test": "npm run lint && npm run test:setup && echo \"Lint only - no other tests yet\"",
    "test:setup": "npm run build"
  }
}
